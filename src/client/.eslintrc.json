{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "plugin:react/recommended",
    "eslint:recommended",
    "google"
  ],
  "plugins": [
    "babel",
    "react"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "google": false,
    "alert": false,
    "css": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "no-console": 0,
    "require-jsdoc": 0,
    "semi": [
      2,
      "never"
    ],
    "no-invalid-this": 0,
    "babel/no-invalid-this": 1
  }
}
