{
  "root": true,
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "plugin:react/recommended", "eslint:recommended"],
  "plugins": ["babel", "react"],
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "google": false,
    "alert": false,
    "css": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "no-console": 0,
    "require-jsdoc": 0,
    "no-invalid-this": 0,
    "babel/no-invalid-this": 0
  }
}
