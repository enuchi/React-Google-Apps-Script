{
  "name": "preact-ts-gas",
  "version": "1.0.0",
  "description": "Starter project for using Preact/Typescript with Google Apps Script",
  "scripts": {
    "test": "test",
    "login": "clasp login",
    "setup": "clasp create --type sheets --title 'My Preact Project' --rootDir ./dist",
    "start": "webpack --mode development",
    "build": "webpack --mode production",
    "deploy": "npm run build && npx clasp push",
    "watch": "npm run build && npx clasp push -w",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "exit 0",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "keywords": [
    "preact",
    "google",
    "apps",
    "script",
    "sheets",
    "typescript"
  ],
  "author": "Elisha Nuchi",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@google/clasp": "^1.7.0",
    "@types/google-apps-script": "0.0.26",
    "@types/node": "^10.0.3",
    "clasp": "^1.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "gas-lib": "^2.0.3",
    "gas-webpack-plugin": "^0.3.0",
    "gts": "^1.1.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "preact": "^10.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "style-loader": "^0.22.1",
    "ts-loader": "^6.2.0",
    "tslint": "^5.20.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "~3.5.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.33.0",
    "webpack-clean": "^1.2.3",
    "webpack-cli": "^3.3.3"
  }
}
